{
    "manifest_version": 2,
    "name": "CELC",
    "version": "1.0",
    
    "icons": {
        "32": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    
    "permissions": [
        "http://celc.devtestnet.com/*",
        "tabs"
    ],
    
    "background": {
        "scripts": ["js/background.js"]
    },
    
    "browser_action": {
        "default_icon": "img/icon.png"
    }
    
}